{
    "builds": [
        {
            "src": "server/python-server.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/python-server.py"
        }
    ],
    "functions": {
        "server/python-server.py": {
            "maxDuration": 120
        }
    }
}